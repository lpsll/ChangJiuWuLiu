<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    android:orientation="vertical">
<include layout="@layout/layout_back_title2"/>
    <LinearLayout
        android:id="@+id/ll_guest_up_door"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/my_item_bg"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="17dp"
            android:layout_weight="1"
            android:text="客户送车上门"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="17dp"
            android:src="@drawable/right_arrow" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:text="长久提车到库"
        android:textColor="@color/black"
        android:textSize="14sp" />
    <LinearLayout

        android:id="@+id/ll_free_send"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/my_item_bg"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="17dp"
            android:layout_weight="1"
            android:text="0公里接送"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="17dp"
            android:src="@drawable/right_arrow" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:text="计算方式："
        android:textSize="12sp"
        android:id="@+id/tv_calc_free_send" />
    <LinearLayout

        android:id="@+id/ll_man_send"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/my_item_bg"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="17dp"
            android:layout_weight="1"
            android:text="人工加送"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="17dp"
            android:src="@drawable/right_arrow" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:text="计算方式："
        android:textSize="12sp"
        android:id="@+id/tv_man_send" />
    <LinearLayout

        android:id="@+id/ll_city_center"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/my_item_bg"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_city_center"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="17dp"
            android:layout_weight="1"
            android:text="北京业务中心"
            android:textColor="@color/black"
            android:textSize="14sp" />

    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:text="北京史各庄"
        android:textSize="12sp"
        android:id="@+id/tv_send_center" />
</LinearLayout>